    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "html-webpack-plugin": "^3.2.0",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.0",
    "morgan": "^1.9.0",
    "node-sass": "^4.9.0",
    "passport": "^0.4.0",
    "passport-google-token": "^0.1.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",

    "react-hot-loader": "^4.3.3",
    "workbox-webpack-plugin": "^3.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chalk": "^2.4.1",
    "concurrently": "^3.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",

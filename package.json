  "scripts": {
    "client:dev": "webpack -d --config webpack.config.development.js --watch --hot",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack -p --config webpack.config.production.js",
    "prestart": "yarn run build",
    "start": "node server",
    "server:dev": "nodemon --watch dist --watch server server",
    "dev": "concurrently --kill-others \"webpack -d --config webpack.config.development.js --watch --hot\" \"yarn run server:dev\""
  },
  "dependencies": {
